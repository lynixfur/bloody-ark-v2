model lethalquests_quests_status {
  SteamId   BigInt @db.UnsignedBigInt
  QuestType Int    @db.TinyInt
  QuestID   BigInt
  TimeStamp BigInt @default(0)
  Completed Int    @default(0) @db.TinyInt

  @@id([SteamId, QuestType, QuestID])
